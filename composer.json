{
    "name": "harderwork/football-data",
    "description": "FootballData plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "harderwork\\football-data\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "harderwork\\football-data\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
